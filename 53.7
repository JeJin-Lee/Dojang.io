#include <stdio.h>
#include <stdlib.h>

struct Person {
    char name[30];
    int age;
};

int main()
{
    struct Person *p[5];

   int cnt=0;
for(int i=0; i<sizeof(p)/sizeof(struct Person*); i++){
    p[i] = malloc(sizeof(struct Person));
    scanf("%s %d", &p[i]->name, &p[i]->age);
}

for(int i=1; i<sizeof(p)/sizeof(struct Person*); i++){
    int max = p[0]->age;
    if(max<p[i]->age){
        max = p[i]->age;
        cnt=i;
    }
}
printf("%s", p[cnt]->name);

    for (int i = 0; i < sizeof(p) / sizeof(struct Person *); i++)
    {
        free(p[i]);
    }

    return 0;
}
